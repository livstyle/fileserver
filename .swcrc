{
    "jsc": {
        "target": "es2020",
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "dynamicImport": true,
            "decorators": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "loose": true,
        "keepClassNames": true
    },
    "module": {
        "type": "es6"
    }
}